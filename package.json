{
	"name": "heartbeat",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"watch": "nodemon ./src/bin/www",
		"devwin": "set NODE_ENV=development&yarn watch",
		"dev": "NODE_ENV=development&yarn watch",
		"test": "echo no test",
		"test:cover": "yarn test -- --coverage ",
		"test:cover:CI": "yarn test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
		"test:watch": "jest --watch",
		"heroku-postbuild": "cd client && yarn && yarn run build",
		"seeder:gauges": "md-seed run Gauges",
		"lint": "eslint --fix **/*.js --ignore-pattern node_modules/"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"babel-eslint": "^8.2.5",
		"bcrypt": "^2.0.1",
		"cookie-parser": "~1.4.3",
		"cors": "^2.8.4",
		"cron": "^1.3.0",
		"csvtojson": "^2.0.4",
		"debug": "~2.6.9",
		"dotenv": "^6.0.0",
		"express": "~4.16.0",
		"http-errors": "~1.6.2",
		"jade": "~1.11.0",
		"jsonwebtoken": "^8.3.0",
		"mongoose": "^5.1.6",
		"mongoose-data-seed": "^1.0.6",
		"morgan": "~1.9.0",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"request": "^2.87.0"
	},
	"devDependencies": {
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.10.0"
	}
}
